for a in range(10):
    print("b"*(a))
for a in range(10):
    print("b"*(a+2))
from random import randint
x= randint(1,10)
print("A random number between 1 and 10: ", x)
from math import sin, pi, cos
print("Pi is roughly ", pi)
print("sin(0)= ", sin(0))
print("cos(0)= ", cos(0))
print(abs(-4.3))
print(round(3.336, 2))
print(round(345.2, -1))
 